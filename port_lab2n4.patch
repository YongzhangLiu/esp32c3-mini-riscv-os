--- /dev/null
+++ b/examples/02-ContextSwitch/riscv.h
@@ -0,0 +1,35 @@
+#ifndef __RISCV_H__
+#define __RISCV_H__
+
+#include <stdint.h>
+
+#define reg_t uint32_t // RISCV32: register is 32bits
+// define reg_t as uint64_t // RISCV64: register is 64bits
+
+// ref: https://www.activexperts.com/serial-port-component/tutorials/uart/
+#define UART        0x10000000
+#define UART_THR    (uint8_t*)(UART+0x00) // THR:transmitter holding register
+#define UART_LSR    (uint8_t*)(UART+0x05) // LSR:line status register
+#define UART_LSR_EMPTY_MASK 0x40          // LSR Bit 6: Transmitter empty; both the THR and LSR are empty
+
+// Saved registers for kernel context switches.
+struct context {
+  reg_t ra;
+  reg_t sp;
+
+  // callee-saved
+  reg_t s0;
+  reg_t s1;
+  reg_t s2;
+  reg_t s3;
+  reg_t s4;
+  reg_t s5;
+  reg_t s6;
+  reg_t s7;
+  reg_t s8;
+  reg_t s9;
+  reg_t s10;
+  reg_t s11;
+};
+
+#endif
diff --git a/examples/02-ContextSwitch/start.s b/examples/02-ContextSwitch/start.s
new file mode 100644
index 0000000..faa47b4
--- /dev/null
+++ b/examples/02-ContextSwitch/start.s
@@ -0,0 +1,16 @@
+.equ STACK_SIZE, 8192
+
+.global _start
+
+_start:
+    csrr a0, mhartid                # 讀取核心代號
+    bnez a0, park                   # 若不是 0 號核心，跳到 park 停止
+    la   sp, stacks + STACK_SIZE    # 0 號核心設定堆疊
+    j    os_main                    # 0 號核心跳到主程式 os_main
+
+park:
+    wfi
+    j park
+
+stacks:
+    .skip STACK_SIZE                # 分配堆疊空間
